## Working Dir: /var/www/code/api/Console
cd /var/www/code/api/Console

# sync items, prompt for stuff along the way
sudo ./cake item_sync syncItems -v
# use this one instead to automate the entire process.
sudo ./cake item_sync syncAutomated dry_run=0,add_missing=1,update_outdated=1,sync_unchanged=1 -v

cd ~
